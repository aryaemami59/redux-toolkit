{
  "name": "@reduxjs/prettier-config",
  "version": "0.0.1",
  "description": "Prettier configuration for Redux projects",
  "source": "./src/prettier.config.mts",
  "types": "./dist/prettier.config.d.mts",
  "module": "./dist/prettier.config.mjs",
  "main": "./dist/prettier.config.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/prettier.config.d.mts",
      "import": "./dist/prettier.config.mjs",
      "default": "./dist/prettier.config.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "prepack": "yarn build"
  },
  "keywords": [
    "prettier",
    "config",
    "prettier-config",
    "reduxjs",
    "redux-toolkit",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reduxjs/redux-toolkit.git",
    "directory": "packages/configs/prettier"
  },
  "bugs": {
    "url": "https://github.com/reduxjs/redux-toolkit/issues"
  },
  "peerDependencies": {
    "prettier": "^3"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "devDependencies": {
    "@reduxjs/tsconfig": "workspace:^",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "sideEffects": false
}
